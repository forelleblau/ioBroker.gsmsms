  "protectedNative": [
    "pin"
  ],
  "encryptedNative": [
    "pin"
  ],
  "native": {
    "port": "",
    "autoDeleteOnReceive": true,
    "enableConcatenation": true,
    "incomingCallIndication": false,
    "incomingSMSIndication": true,
    "getSignalQuality": false,
    "pin": "",
    "customInitCommand": "",
    "cnmiModemOpen": "2,1,0,2,0",
    "cnmiModemClosed": "2,0,2,2,1",
    "baudRate": 19200,
    "dataBits": 8,
    "stopBits": 1,
    "parity": "none",
    "rtscts": false,
    "xon": false,
    "xoff": false,
    "xany": false,
    "connectionMode": "alwaysopen",
    "pollinginterval": 30,
    "defaultrecipientnotifications": ""
  },
