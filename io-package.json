      "native": {}
    },
    {
      "_id": "admin.ownName",
      "type": "state",
      "common": {
        "name": "Own Name",
        "type": "string",
        "options": {
          "maxLength": 17
        },
        "role": "text",
        "read": true,
        "write": true,
        "desc": "Name to SIM-card number (sender)"
      },
      "native": {}
    },
    {
      "_id": "admin.ownNumber",
      "type": "state",
      "common": {
        "name": "Own Number",
        "type": "string",
